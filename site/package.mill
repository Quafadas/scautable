package build.site

import mill._, scalalib._
import mill.scalajslib.api._
import io.github.quafadas.millSite._
import mill.util.VcsVersion

object `package` extends SiteModule {
  def latestVersion: T[String] = Task {
    VcsVersion.vcsState().lastTag.getOrElse("0.0.0").replace("v", "")
  }

  override def unidocTitle = Task("Scautable API")
  
  override def unidocDeps = Seq(build.scautable.jvm)
}